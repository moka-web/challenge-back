{
  "moduleFileExtensions": ["ts", "js", "json"],
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/../src/$1",
    "^(\\.{1,2}/.*)\\.js$": "$1"
  },
  "extensionsToTreatAsEsm": [],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": ["ts-jest", {
      "tsconfig": {
        "module": "commonjs",
        "moduleResolution": "node",
        "resolvePackageJsonExports": false,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true
      }
    }]
  },
  "testTimeout": 30000
}
